{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": [
          "https://apis.mapmyindia.com/advancedmaps/v1/4877ca98884eb8a81788d9435ddcc3ca/map_load?v=1.3"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data",
      "urls": [
        "https://oxyplus-api.mybluemix.net/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "12h",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
